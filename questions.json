{
  "step1_inbound": [
    {
      "type": "text",
      "label": "Client Name",
      "name": "contact_name"
    },
    {
      "type": "select",
      "label": "Call Type (Inbound)",
      "name": "call_type_ctm",
      "options": [
        "New Project",
        "Existing Project",
        "Vendor",
        "Technician",
        "Complaint",
        "Promotion/Spam",
        "HR Inquiry",
        "Unknown"
      ]
    },
    {
      "type": "textarea",
      "label": "Basic request/description",
      "name": "basic_req"
    },
    {
      "type": "select",
      "label": "Qualification (Optional)",
      "name": "qualification_ctm",
      "options": [
        "",
        "Qualified",
        "Not Qualified"
      ]
    }
  ],

  "step1_outbound": [
    {
      "type": "text",
      "label": "Outbound Client Name",
      "name": "outbound_client_name"
    },
    {
      "type": "text",
      "label": "Phone or ID",
      "name": "outbound_phone"
    },
    {
      "type": "textarea",
      "label": "Reason for outbound call",
      "name": "outbound_reason"
    }
  ],

