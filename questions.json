{
  "step1_inbound": [
    {
      "type": "text",
      "label": "Client Name",
      "name": "contact_name"
    },
    {
      "type": "select",
      "label": "Call Type",
      "name": "call_type_ctm",
      "options": [
        "New Project",
        "Existing Project",
        "Vendor",
        "Technician",
        "Complaint",
        "Promotion/Sales/Spam Calls",
        "HR Inquiry",
        "Unknown"
      ]
    },
    {
      "type": "select",
      "label": "Which Company Location?",
      "name": "company_location_ctm",
      "options": [
        "Orlando","San Francisco","Las Vegas","Atlanta","New York",
        "Los Angeles","Houston","Phoenix","Chicago","Miami"
      ]
    },
    {
      "type": "checkbox-multi",
      "label": "Which Services?",
      "name": "requested_services_ctm",
      "options": [
        "Stage","Audio","TVs","Projections & Screens","LED Wall",
        "Live Stream","Step & Repeat","Lighting","Pipe & Drape",
        "Karaoke","Outdoor Movie","Full Event Production","Other"
      ]
    },
    {
      "type": "select",
      "label": "Qualification (Optional)",
      "name": "qualification_ctm",
      "options": ["","Qualified","Not Qualified"]
    }
  ],

  "step1_outbound": [
    {
      "type": "text",
      "label": "Outbound Client Name",
      "name": "outbound_client_name"
    },
    {
      "type": "text",
      "label": "Outbound Phone/ID",
      "name": "outbound_phone"
    }
  ],

  "step2_newProject": [
    {
      "type": "date",
      "label": "Event Date",
      "name": "event_date_ctm"
    },
    {
      "type": "text",
      "label": "Event Name",
      "name": "event_name_ctm"
    },
    {
      "type": "text",
      "label": "Event Street (Google Maps)",
      "name": "event_street_ctm"
    },
    {
      "type": "text",
      "label": "City",
      "name": "event_city_ctm"
    },
    {
      "type": "text",
      "label": "State",
      "name": "event_state"
    },
    {
      "type": "select",
      "label": "Delivery & Setup Options",
      "name": "delivery_setup_options_ctm",
      "options": ["Delivery","Setup","Operation","Pick Up Yourself"],
      "multi": true
    }
  ],

  "step2_existing": [
    {
      "type": "text",
      "label": "Rental Order Number",
      "name": "rental_order_number_ctm"
    }
  ],

  "step2_vendor": [
    {
      "type": "text",
      "label": "Vendor Company Name",
      "name": "vendor_company_name_ctm"
    }
  ],

  "step2_technician": [
    {
      "type": "text",
      "label": "Project #",
      "name": "tech_order_number"
    }
  ],

  "step2_complaint": [
    {
      "type": "text",
      "label": "Complaint Project #",
      "name": "complaint_order_number"
    }
  ],

  "step2_hr": [
    {
      "type": "text",
      "label": "Candidate Name / Inquiry?",
      "name": "hr_candidate_name"
    }
  ],

  "step2_unknown": [
    {
      "type": "textarea",
      "label": "Any notes for unknown request?",
      "name": "unknown_notes"
    }
  ],

  "step3_outbound": [
    {
      "type": "textarea",
      "label": "Any additional notes? (Outbound follow-up)",
      "name": "outbound_moreinfo"
    }
  ],

  "step2p5_salesAdvanced": [
    {
      "type": "select",
      "label": "Budget Range?",
      "name": "sales_budget",
      "options": ["Under $2k","$2k-$5k","$5k-$10k","$10k-$20k","$20k+"]
    }
  ],

  "stepPreview": [
    {
      "type": "text",
      "label": "Preview summary",
      "name": "_summary_"
    }
  ],

  "stepFinal": [
    {
      "type": "select",
      "label": "Final Qualification (required)",
      "name": "qualification_ctm",
      "options": ["Qualified","Not Qualified"]
    }
  ]
}
