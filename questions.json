{
  "part1": [
    {
      "type": "text",
      "label": "Client's Full Name",
      "name": "clientName",
      "placeholder": "Enter client name"
    },
    {
      "type": "text",
      "label": "Phone Number",
      "name": "phoneNumber",
      "placeholder": "XXX-XXX-XXXX"
    },
    {
      "type": "text",
      "label": "Email",
      "name": "email",
      "placeholder": "client@example.com"
    },
    {
      "type": "text",
      "label": "Service / Product Interested In",
      "name": "serviceInterested",
      "placeholder": "e.g. Car rental, Boat rental, etc."
    },
    {
      "type": "textarea",
      "label": "Describe the Request",
      "name": "requestDescription",
      "placeholder": "Short summary of what the client needs"
    },
    {
      "type": "radio",
      "label": "Is this a first-time client?",
      "name": "isFirstTime",
      "options": ["Yes", "No"]
    },
    {
      "type": "text",
      "label": "Preferred Date / Time",
      "name": "preferredDateTime",
      "placeholder": "e.g. 03/10/2025 14:00"
    }
  ],

  "part2": [
    {
      "type": "radio",
      "label": "Do we have the budget range?",
      "name": "hasBudget",
      "options": ["Yes", "No"]
    },
    {
      "type": "text",
      "label": "Budget (if known)",
      "name": "budgetValue",
      "placeholder": "e.g. $1000-$2000"
    },
    {
      "type": "radio",
      "label": "Has Payment been confirmed?",
      "name": "paymentConfirmed",
      "options": ["Yes", "No"]
    },
    {
      "type": "select",
      "label": "Lead Status",
      "name": "leadStatus",
      "options": [
        "New Lead",
        "In Progress",
        "Qualified",
        "Follow-up Needed",
        "Closed/Lost"
      ]
    },
    {
      "type": "textarea",
      "label": "Internal Comments (Qualification)",
      "name": "qualificationComments",
      "placeholder": "Any notes from the call center / qualifier?"
    }
  ],

  "part3": [
    {
      "type": "textarea",
      "label": "Full Requirements / Details",
      "name": "fullRequirements",
      "placeholder": "List all details client provided"
    },
    {
      "type": "select",
      "label": "Product/Service Category",
      "name": "productCategory",
      "options": [
        "Car Rental",
        "Yacht Rental",
        "Beauty Appointment",
        "Marketing Consulting",
        "Other"
      ]
    },
    {
      "type": "checkbox",
      "label": "Client Agreed to Terms & Conditions?",
      "name": "agreedToTerms"
    },
    {
      "type": "radio",
      "label": "Need Additional Documents?",
      "name": "needDocs",
      "options": ["Yes", "No"]
    },
    {
      "type": "textarea",
      "label": "CELS Comments",
      "name": "celsComments",
      "placeholder": "Any internal remarks from CELS side"
    }
  ],

  "part4": [
    {
      "type": "radio",
      "label": "Is the client ready to finalize?",
      "name": "readyToFinalize",
      "options": ["Yes", "No"]
    },
    {
      "type": "text",
      "label": "Quoted Price",
      "name": "quotedPrice",
      "placeholder": "e.g. $2,500"
    },
    {
      "type": "textarea",
      "label": "Additional Follow-up Notes",
      "name": "followUpNotes",
      "placeholder": "Summarize next steps or final arrangement"
    }
  ]
}
